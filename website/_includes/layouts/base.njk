<script>

  let settings;

  if(!getCookie("mode") || getCookie("mode") === 'mainnet') {
    document.getElementById("cookie").innerText = "Switch to Testnet"
    settings = getSettings("mainnet");
  }
  else {
    document.getElementById("cookie").innerText = "Switch to Mainnet"
    settings = getSettings("testnet");
  }

  var a = document.getElementById('myLink');
  a.href = settings.opensea + "/collection/" + settings.opensea_collection;

  function getCookie(name) {
    let nameEQ = name + "=";
    let ca = document.cookie.split(';');
    for(let i = 0; i < ca.length; i++) {
      let c = ca[i];
      while (c.charAt(0) === ' ') {
        c = c.substring(1, c.length);
      }
      if (c.indexOf(nameEQ) === 0) {
        return c.substring(nameEQ.length, c.length);
      }
    }
    return null;
  }

  function setCookie(name, value) {
    document.cookie = name + "=" + (value || "") + "; path=/";
  }

  function switchNet() {
    if(!getCookie("mode") || getCookie("mode") === 'mainnet') {
      setCookie("mode", "testnet");
    }
    else {
      setCookie("mode", "mainnet");
    }
    location.reload();
  }

  function getSettings(mode) {
    switch (mode) {
      case "mainnet":
        return {
          opensea: '{{ mainnet.opensea }}',
          opensea_collection: '{{ mainnet.opensea_collection }}',
          contract: '{{ mainnet.contract }}',
          network: '{{ mainnet.network }}',
          network_name: '{{ mainnet.network_name }}',
          proofi: '{{ mainnet.proofi }}'
      }
      case "testnet":
        return {
          opensea: '{{ testnet.opensea }}',
          opensea_collection: '{{ testnet.opensea_collection }}',
          contract: '{{ testnet.contract }}',
          network: '{{ testnet.network }}',
          network_name: '{{ testnet.network_name }}',
          proofi: '{{ testnet.proofi }}'
      }
    }
  }



</script>

